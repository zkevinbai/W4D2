<h1>Cats#index</h1>
<!-- <p>Find me in app/views/cats/index.html.erb</p> -->

<ol>
  <% @cats.each do |cat| %>
    <li>
      <ul>
        <li><%= cat.name %></li>
        <li><%= cat.age %></li>
        <li><%= cat.birth_date %></li>
        <li><%= cat.color %></li>
        <li><%= cat.sex %></li>
        <li><%= cat.description %></li>
      </ul>
      <br>
    </li>
  <% end %>
</ol>